{
    "presets": [
      ["es2015", {"modules": false}], "react"
    ],
    
    "plugins": [
      "react-hot-loader/babel",
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    
    "env": {
      "test": {
        "plugins": ["transform-es2015-modules-commonjs"]
      },
      "development": {
        "presets": ["react-hmre"]
      }
    }
}